# DeepSeek-OCR 환경 설정
# M1 Mac 기본 설정

# 디바이스 설정 (cpu/mps/cuda)
DEEPSEEK_DEVICE=mps

# 데이터 타입 (float32/float16/bfloat16)
# M1 Mac은 MPS에서 float32 권장
DEEPSEEK_DTYPE=float32

# 배치 크기 (M1 8GB 메모리 기준)
DEEPSEEK_BATCH_SIZE=1

# 모델 경로
DEEPSEEK_MODEL_PATH=./models/deepseek-ocr

# 출력 디렉토리
DEEPSEEK_OUTPUT_DIR=./deepseek_test/outputs

# 로깅 레벨 (DEBUG/INFO/WARNING/ERROR)
LOG_LEVEL=INFO
